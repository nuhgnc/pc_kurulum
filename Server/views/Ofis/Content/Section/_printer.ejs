<!--Varsayılan yazıcı var mı? -->

    <% defaultprinter = printer_status.filter(item => item.default == true ) %>

<div class="flex flex-wrap border-b pb-8 ">
        <div class="w-full md:w-1/2 xl:w-1/4 p-6 flex-1">
            <!--Metric Card-->
            <div class="<%- sections.printers.style.bg %> ">
                <div class="flex flex-row items-center" data-popover-target="popover-default-1992" type="button">
                    <div class="flex-shrink pr-4">
                        <div class="rounded-full p-5 bg-green-600"><i><img src="<%- sections.printers.img %>" alt="" srcset=""></i></div>
                    </div>
                    <div class="flex-1 text-right md:text-center">
                        <h5 class="font-bold uppercase text-gray-600">Varsayılan yazıcı</h5>
                        <h3  class="font-bold text-xl"> <%- defaultprinter[0].name %> <span class="text-green-500"></i></span></h3>
                    </div>
                </div>
            </div>
            <!--/Metric Card-->
        </div>
        <!-- Popover -->
        <div data-popover id="popover-default-1992" role="tooltip" class="absolute z-10 invisible inline-block w-64 text-sm text-gray-500 transition-opacity duration-300 bg-white border border-gray-200 rounded-lg shadow-sm opacity-0 dark:text-gray-400 dark:border-gray-600 dark:bg-gray-800">
            <div class="px-3 py-2 bg-gray-100 border-b border-gray-200 rounded-t-lg dark:border-gray-600 dark:bg-gray-700">
                <h3 class="font-semibold text-gray-900 dark:text-white"> Varsayılan yazıcı </h3>
            </div>
            <div class="px-3 py-2">
                <p> <%- defaultprinter[0].model %> </p>
            </div>
            <div data-popper-arrow></div>
        </div>

        <div class="w-full md:w-1/2 xl:w-1/4 p-6">
            <!--Metric Card-->
            <div class="<%- sections.printers.style.bg %> ">
                <div class="flex flex-row items-center" data-popover-target="popover-default-yazıcıSayi" type="button" >
                    <div class="flex-shrink pr-4">
                        <div class="rounded-full p-5 bg-green-600"><i><img src="<%- sections.printers.img %>" alt="" srcset=""></i></div>
                    </div>
                    <div class="flex-1 text-right md:text-center">
                        <h5 class="font-bold uppercase text-gray-600">Yüklü yazıcı</h5>
                        <h3 class="font-bold text-xl"> <%- printer_status.length %> adet<span class="text-green-500"><i class=""></i></span></h3>
                    </div>
                </div>
            </div>
            <!--/Metric Card-->
        </div>
        <!-- Popover -->
        <div data-popover id="popover-default-yazıcıSayi" role="tooltip" class="absolute z-10 invisible inline-block w-64 text-sm text-gray-500 transition-opacity duration-300 bg-white border border-gray-200 rounded-lg shadow-sm opacity-0 dark:text-gray-400 dark:border-gray-600 dark:bg-gray-800">
            <div class="px-3 py-2 bg-gray-100 border-b border-gray-200 rounded-t-lg dark:border-gray-600 dark:bg-gray-700">
                <h3 class="font-semibold text-gray-900 dark:text-white"> Yüklü yazıcılar </h3>
            </div>
            <div class="px-3 py-2">
                <p> <% printer_status.forEach(element => { %>
                       <%- element.name %> <br>
                <% }) %> </p>
            </div>
            <div data-popper-arrow></div>
        </div>
</div>